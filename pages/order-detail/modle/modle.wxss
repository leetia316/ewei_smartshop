/* modle.wxss */

.citypicker{
    width: 100%;
    height: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,0.4);
    z-index: 103;
}
.citybody {
    width: 100%;
    height: 542rpx;
    background: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 106;
}

.cityheader {
    width: 100%;
    position: absolute;
    top:0;
    left: 0;
    z-index: 107;
}

.city-cancel {
    float: left;
    margin: 20rpx;
    color: #333;
    font-size: 36rpx;
}

.city-true {
    float: right;
    margin: 20rpx;
    color: #FF9C00;
    font-size: 36rpx;
}

