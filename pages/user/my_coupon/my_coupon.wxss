  margin-right: 14rpx;
}
.myCouponContainer .bannerWrap .bannerContent .rightArrowArr .arrow {
  display: inline-block;
  height: 19rpx;
  width: 19rpx;
  box-sizing: border-box;
  border-right: 2rpx solid #fff;
  border-top: 2rpx solid #fff;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
  margin-right: -4rpx;
}
.myCouponContainer .bannerWrap .bannerContent .rightArrowArr .arrow.lightArrow {
